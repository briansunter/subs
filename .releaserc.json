{
  "branches": ["main", "master"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/npm",
    "@semantic-release/github",
    "@semantic-release/git"
  ],
  "preset": "conventionalcommits",
  "commitUrlFormat": "https://github.com/briansunter/subs/commits/{{hash}}",
  "compareUrlFormat": "https://github.com/briansunter/subs/compare/{{previousTag}}...{{currentTag}}",
  "issueUrlFormat": "https://github.com/briansunter/subs/issues/{{id}}",
  "changelogFile": "CHANGELOG.md"
}
